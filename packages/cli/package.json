{
  "name": "@webrenew/unicon",
  "version": "0.1.1",
  "description": "CLI for searching and bundling icons from Unicon - the unified icon library",
  "type": "module",
  "bin": {
    "unicon": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "prepublishOnly": "npm run build",
    "publish:package": "./scripts/publish.sh",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "lucide",
    "phosphor",
    "hugeicons",
    "cli",
    "unicon"
  ],
  "author": "WebRenew",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WebRenew/unicon.git",
    "directory": "packages/cli"
  },
  "homepage": "https://unicon.dev",
  "bugs": {
    "url": "https://github.com/WebRenew/unicon/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@types/figlet": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "figlet": "^1.9.4",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
